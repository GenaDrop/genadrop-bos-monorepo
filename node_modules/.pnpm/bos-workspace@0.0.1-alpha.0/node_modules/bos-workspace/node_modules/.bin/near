#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Documents/POW/shot/monorepo/node_modules/.pnpm/near-cli-rs@0.4.3/node_modules/near-cli-rs/node_modules:/Users/mac/Documents/POW/shot/monorepo/node_modules/.pnpm/near-cli-rs@0.4.3/node_modules:/Users/mac/Documents/POW/shot/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Documents/POW/shot/monorepo/node_modules/.pnpm/near-cli-rs@0.4.3/node_modules/near-cli-rs/node_modules:/Users/mac/Documents/POW/shot/monorepo/node_modules/.pnpm/near-cli-rs@0.4.3/node_modules:/Users/mac/Documents/POW/shot/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../near-cli-rs@0.4.3/node_modules/near-cli-rs/run.js" "$@"
else
  exec node  "$basedir/../../../../../near-cli-rs@0.4.3/node_modules/near-cli-rs/run.js" "$@"
fi
